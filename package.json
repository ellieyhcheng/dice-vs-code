{
    "name": "dice-vs-code",
    "displayName": "Dice Syntax Highlighter",
    "description": "Dice syntax highlighting",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.49.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "publisher": "ellieyhc",
    "repository": {"url": "https://github.com/ellieyhcheng/dice-vs-code"},
    "contributes": {
        "languages": [{
            "id": "dice",
            "aliases": ["Dice", "dice"],
            "extensions": [".dice"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "dice",
            "scopeName": "source.dice",
            "path": "./syntaxes/dice.tmLanguage.json"
        }]
    }
}